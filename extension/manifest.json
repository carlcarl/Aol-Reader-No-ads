{
	"name": "Aol Reader No-ads",
	"version": "1.4",
	"manifest_version": 2,

	"description": "Hide the ads and expand the content area in Aol reader.",
	"developer": {
		"name": "carlcarl",
		"url": "http://blog.carlcarl.tw"
	},
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"content_scripts": [
		{
			"matches": ["http://reader.aol.com/*"],
			"js": ["aol.user.js"]
		}
	]
}

